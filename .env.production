# ============================================
# 生产环境配置文件
# ============================================

# API配置
# 生产环境直接请求后端代理服务
VITE_API_BASE_URL=https://chat.toproject.cloud

# SSH配置
VITE_SSH_DEFAULT_HOST=localhost
VITE_SSH_DEFAULT_PORT=22
VITE_SSH_WEBSOCKET_URL=wss://wss.toproject.cloud/ws/ssh
VITE_SSH_EXECUTE_URL=wss://wss.toproject.cloud/ws/ssh/execute

# SSH轮训配置 - 用于SSH连接的WebSocket服务器列表
VITE_SSH_WEBSOCKET_DOMAINS="wss://wss.toproject.cloud/ws/ssh"

# 应用配置
VITE_APP_TITLE=ZenReader
VITE_APP_DESCRIPTION=多功能在线工具集合

# 认证配置
VITE_AUTH_ENABLED=false
VITE_AUTH_TOKEN_KEY=zenreader_token

# ============================================
# 各模型 API Token 配置 (生产环境请填写实际值)
# ============================================

